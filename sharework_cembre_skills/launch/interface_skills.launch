<arg name="pipeline" default="dirrt" />
<arg name="load_param" default="load" />
<arg name="debug" default="false" />
<arg name="baseline" default="false"/>

<rosparam command="load" file="$(find sharework_cembre_skills)/config/multi_skills.yaml" />

<include file="$(find sharework_cembre_skills)/launch/interface_pick.launch">
<arg name="debug" value="$(arg debug)"/>
<arg name="load_param" value="$(arg load_param)"/>
<arg name="pipeline" value="$(arg pipeline)"/>
<arg name="baseline" value="$(arg baseline)"/>
</include>


<include file="$(find sharework_cembre_skills)/launch/interface_place.launch">
<arg name="debug" value="$(arg debug)"/>
<arg name="load_param" value="$(arg load_param)"/>
<arg name="pipeline" value="$(arg pipeline)"/>
<arg name="baseline" value="$(arg baseline)"/>
</include>


<include file="$(find sharework_cembre_skills)/launch/interface_goto.launch">
<arg name="debug" value="$(arg debug)"/>
<arg name="load_param" value="$(arg load_param)"/>
<arg name="pipeline" value="$(arg pipeline)"/>
<arg name="baseline" value="$(arg baseline)"/>
</include>

</launch>
